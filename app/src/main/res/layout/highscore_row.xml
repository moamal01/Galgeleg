<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="30dp"
    xmlns:tools="http://schemas.android.com/tools"
    tools:background="#000">

    <TextView
        android:id="@+id/name_highScore"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Navn"
        android:textColor="#FFF"
        android:textSize="20dp"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toLeftOf="@id/tries_highScore"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/tries_highScore"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="75dp"
        android:text="Antal Forkerte gÃ¦t"
        android:textColor="#FFF"
        android:textSize="20dp"
        app:layout_constraintLeft_toRightOf="@id/name_highScore"
        app:layout_constraintTop_toTopOf="parent" />


    <TextView
        android:id="@+id/word_highScore"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="75dp"
        android:text="Ord"
        android:textColor="#FFF"
        android:textSize="20dp"
        app:layout_constraintLeft_toRightOf="@id/tries_highScore"
        app:layout_constraintTop_toTopOf="parent" />

</androidx.constraintlayout.widget.ConstraintLayout>